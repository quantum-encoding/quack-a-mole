# Quack-a-Mole Environment Configuration
# Copy this file to .env and update with your values

# Backend Configuration
QUACK_AMPLITUDE=11
QUANTUM_STATE=superposition
MOLE_DETECTION=true
POND_CAPACITY=1000000
WATER_PHYSICS=navier-stokes

# ML Service Configuration
TENSORFLOW_THREADS=4
QUANTUM_COHERENCE=0.73
PARALLEL_UNIVERSES=42
DUCK_CONFIDENCE_THRESHOLD=0.8
MOLE_PARANOIA_LEVEL=0.6

# Rust Service Configuration
HAMMER_TYPE=foam
SIMD_ENABLED=false
WHACK_SPEED=ludicrous
MOLE_GRID_SIZE=100

# Frontend Configuration
REACT_APP_API_URL=http://localhost:8080
REACT_APP_ML_API_URL=http://localhost:5000
REACT_APP_RUST_API_URL=http://localhost:3030
REACT_APP_WEBSOCKET_URL=ws://localhost:8080/ws

# Database (Future Enhancement)
# DB_HOST=localhost
# DB_PORT=5432
# DB_NAME=pond
# DB_USER=duck_admin
# DB_PASSWORD=qu4ck_s3cr3t

# Monitoring
ENABLE_METRICS=true
METRICS_PORT=9090
LOG_LEVEL=info

# Security
API_KEY=YOUR_DUCK_API_KEY_HERE
JWT_SECRET=YOUR_SUPER_SECRET_QUACK_KEY
ENABLE_CORS=true

# Feature Flags
ENABLE_QUANTUM_TUNNELING=true
ENABLE_EMERGENCY_QUACKEN=false
ENABLE_DUCK_ENTANGLEMENT=true
ENABLE_MOLE_PREDICTION=true

# Performance
MAX_CONCURRENT_DUCKS=10000
QUACK_CACHE_TTL=300
MOLE_DETECTION_INTERVAL=1000

# Easter Eggs
KONAMI_CODE_ENABLED=true
QUACKEN_RELEASE_CODE=release-the-quacken